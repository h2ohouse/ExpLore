    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-bootstrap": "^1.0.0-beta.8",
    "update": "^0.7.4",
